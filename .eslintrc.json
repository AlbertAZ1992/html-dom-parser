{
  "env": {
    "browser": true,
    "commonjs": true,
    "mocha": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 6
  },
  "plugins": ["prettier"],
  "rules": {
    "no-debugger": "error",
    "no-console": "error",
    "prettier/prettier": "error"
  }
}
