{
  "compilerOptions": {
    "module": "commonjs",
    "lib": ["es6", "dom"],
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": { "html-dom-parser": ["."], "html-dom-parser/*": ["./*"] }
  }
}
